{
  "ignorePatterns": [
      {
        "pattern": "(localhost|my\\.massa)"
      },
      {
        "pattern": "^http://manpages.ubuntu.com"
      }
    ],
    "replacementPatterns": [
      {
        "pattern": "\\./",
        "replacement": "file://docs"
      },
      {
        "pattern": "^/",
        "replacement": "https://docs.massa.net/"
      }
    ],
  "timeout": "20s",
  "retryOn429": true,
  "retryCount": 5,
  "fallbackRetryDelay": "30s",
  "aliveStatusCodes": [
    200,
    206
  ]
}