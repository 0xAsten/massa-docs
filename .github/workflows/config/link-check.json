{
  "ignorePatterns": [
      {
        "pattern": "(localhost|my\\.massa)"
      },
      {
        "pattern": "^http://manpages.ubuntu.com"
      },
      {
        "pattern": "^https://test.massa.net"
      },
      {
        "pattern": "^https://buildnet.massa.net"
      }
    ],
    "replacementPatterns": [
      {
        "pattern": "^/",
        "replacement": "{{BASEURL}}"
      }
    ],
  "timeout": "20s",
  "retryOn429": true,
  "retryCount": 5,
  "fallbackRetryDelay": "30s",
  "aliveStatusCodes": [
    200,
    206
  ]
}